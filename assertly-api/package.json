{
  "name": "assertly-api",
  "version": "0.1.0",
  "description": "Assertly API",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "npm run tslint && npm run build-ts",
    "serve": "node dist/server.js",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc && cg",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prettier": "prettier --write"
  },
  "dependencies": {
    "acorn": "^7.3.1",
    "debug": "^4.1.1",
    "express": "^4.16.2",
    "jest": "^26.0.1",
    "ts-api": "^3.4.5",
    "ts-jest": "^26.0.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.10.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.0.0",
    "@types/node": "^14.0.4",
    "@types/prettier": "^2.0.0",
    "babylon": "^7.0.0-beta.47",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.5",
    "recast": "^0.19.1",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  }
}
