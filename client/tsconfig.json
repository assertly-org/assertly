{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es6", "dom"],
      "allowJs": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": false,
      "sourceMap": true      
    },
    "include": [
      "./src/client.ts"
    ]
  }
