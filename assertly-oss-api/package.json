{
  "name": "assertly-oss-api",
  "version": "0.1.0",
  "description": "Assertly API",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "npm run tslint && npm run build-ts",
    "serve": "node dist/server.js",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc && cg",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prettier": "prettier --write",
    "cy": "cypress open"
  },
  "dependencies": {
    "cypress": "^3.8.3",
    "debug": "4.0.1",
    "express": "^4.16.2",
    "jest": "^23.6.0",
    "ts-api": "^3.4.5",
    "ts-jest": "^26.0.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.0.0",
    "@types/node": "^9.6.56",
    "@types/prettier": "^1.19.1",
    "concurrently": "^3.5.1",
    "nodemon": "^2.0.0",
    "prettier": "^1.19.1",
    "ts-node": "^5.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.9.2"
  }
}
