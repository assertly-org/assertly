{
  "name": "assertly-oss-api",
  "version": "0.1.0",
  "description": "Assertly API",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "npm run tslint && npm run build-ts",
    "serve": "node dist/server.js",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc && cg",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prettier": "prettier --write",
    "cy": "cypress open"
  },
  "dependencies": {
    "cypress": "^3.6.1",
    "debug": "4.0.1",
    "express": "^4.16.2",
    "jest": "^23.6.0",
    "ts-api": "^3.3.1",
    "ts-jest": "^22.4.6"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "^4.17.2",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.55",
    "@types/prettier": "^1.19.0",
    "concurrently": "^3.5.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "ts-node": "^5.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
